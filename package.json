{
    "name": "sep490-g55-automation-antidetect-browser",
    "version": "1.0.0",
    "description": "Automation Antidetect Browser - SEP490 G55 Team Project",
    "main": "src/main/index.js",
    "scripts": {
        "dev": "concurrently -k -n \"VITE,ELECTRON\" -c \"cyan,magenta\" \"npm run dev:vite\" \"npm run dev:electron\"",
        "dev:vite": "vite",
        "dev:electron": "wait-on http://localhost:5173 && cross-env NODE_ENV=development electronmon .",
        "build": "vite build && electron-builder",
        "start": "electron .",
        "test": "jest",
        "lint": "eslint src --ext .js,.jsx"
    },
    "keywords": [
        "electron",
        "react",
        "antidetect",
        "browser",
        "automation",
        "playwright"
    ],
    "author": "SEP490 G55 Team",
    "license": "MIT",
    "devDependencies": {
        "@vitejs/plugin-react": "^5.1.0",
        "concurrently": "^9.2.1",
        "cross-env": "^10.1.0",
        "electron": "^39.1.0",
        "electron-builder": "^26.0.12",
        "electronmon": "^2.0.4",
        "eslint": "^8.57.0",
        "jest": "^29.7.0",
        "vite": "^7.2.1",
        "wait-on": "^9.0.1"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.19.2",
        "playwright": "^1.57.0",
        "proxy-chain": "^2.5.0",
        "react": "^19.2.0",
        "react-dom": "^19.2.0"
    }
}