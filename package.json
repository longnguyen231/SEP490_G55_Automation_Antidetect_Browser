{
  "name": "obtautomationantidetectbrowser",
  "version": "1.0.0",
  "description": "Browser profile management with antidetect capabilities using Electron and React",
  "main": "src/main/bootstrap.js",
  "scripts": {
    "dev": "concurrently -k -n \"VITE,ELECTRON\" -c \"cyan,magenta\" \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
  "dev:electron": "wait-on http://localhost:5173 && cross-env NODE_ENV=development electronmon --watch src/main --watch src/preload --ignore dist --ignore release --ignore node_modules --ignore vendor --ignore data --ignore data/** --ignore logs --ignore src/data --ignore src/data/** --ignore src/data/logs --ignore src/data/logs/** --ignore **/*.json .",
    "prebuild": "node scripts/ensure-fsevents.js",
    "icons": "node scripts/make-icons.js",
    "build": "npm run icons && vite build && electron-builder",
    "build:vite": "vite build",
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "electronmon": {
    "watch": [
      "src/main",
      "src/preload"
    ],
    "ignore": [
      "dist",
      "release",
      "node_modules",
      "data",
      "data/**",
      "vendor",
      "logs",
      "src/data",
      "src/data/**",
      "src/data/logs",
      "src/data/logs/**"
    ]
  },
  "keywords": [
    "electron",
    "react",
    "browser",
    "antidetect",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "com.obt.antidetectbrowser",
    "productName": "OBT Antidetect Browser",
    "asar": true,
    "directories": {
      "output": "release"
    },
    "extraResources": [
      {
        "from": "build/1.png",
        "to": "icon.png"
      }
    ],
    "icon": "build/1.png",
    "files": [
      "dist/**/*",
      "src/main/**/*",
      "src/preload/**/*",
      "package.json",
      "!node_modules/**/fsevents",
      "!node_modules/**/fsevents/**"
    ],
    "npmRebuild": false,
    "nodeGypRebuild": false,
    "mac": {
      "category": "public.app-category.developer-tools",
      "extraResources": [
        {
          "from": "vendor/chromium-mac/Chromium.app",
          "to": "Chromium.app",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "win": {
      "target": "nsis",
      "extraResources": [
        {
          "from": "vendor/chrome-win",
          "to": "chrome",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "linux": {
      "target": "AppImage",
      "extraResources": [
        {
          "from": "vendor/chrome-linux",
          "to": "chrome",
          "filter": [
            "**/*"
          ]
        }
      ]
    },
    "afterPack": "./scripts/afterPack-obfuscate.js"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.1.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "electronmon": "^2.0.4",
    "javascript-obfuscator": "^4.1.0",
    "@electron/asar": "^3.2.10",
    "vite": "^7.2.1",
    "wait-on": "^9.0.1",
    "png-to-ico": "^2.1.8"
  },
  "dependencies": {
    "playwright": "^1.48.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "proxy-chain": "^2.5.0",
    "swagger-ui-express": "^5.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "node-cron": "^3.0.3"
  }
}